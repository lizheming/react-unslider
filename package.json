{
  "name": "react-unslider",
  "version": "0.1.0",
  "description": "react carousel component inspired by unslider.js",
  "main": "lib/index.js",
  "directories": {
    "example": "examples"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "scripts": {
    "start": "webpack-dev-server --config=examples/webpack.config.js",
    "build": "webpack --mode=development --config=build/webpack.config.js",
    "build:prod": "webpack --mode=production --config=build/webpack.config.js",
    "build:example": "webpack --config=examples/webpack.config.js",
    "build:lib": "webpack --config=build/webpack.component.config.js",
    "compile": "npm run build && npm run build:prod && npm run build:lib && npm run build:example && npm run build:umd",
    "build:umd": "webpack --config=umd/webpack.config.js",
    "clean": "rimraf lib dist umd/dist",
    "prepublish": "npm run clean && npm run compile && npm run upload:qstatic"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lizheming/react-unslider.git"
  },
  "keywords": [
    "react",
    "unslider",
    "carousel"
  ],
  "author": "lizheming <i@imnerd.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lizheming/react-unslider/issues"
  },
  "homepage": "https://github.com/lizheming/react-unslider#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "css-loader": "^3.4.1",
    "es3ify-webpack-plugin": "^0.1.0",
    "open-browser-webpack-plugin": "0.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.0",
    "style-loader": "^1.1.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  }
}
